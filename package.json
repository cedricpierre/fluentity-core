{
  "name": "@fluentity/core",
  "version": "1.2.6",
  "description": "Fluentity core is a fluent, model-oriented, typed HTTP client for TypeScript.",
  "main": "./dist/fluentity.js",
  "types": "./dist/fluentity.d.ts",
  "type": "module",
  "keywords": [
    "fluentity",
    "rest",
    "api",
    "client",
    "typescript",
    "model",
    "fluent",
    "http",
    "orm"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./examples": {
      "types": "./examples/models/index.d.ts",
      "import": "./examples/models/index.js"
    }
  },
  "packageManager": "pnpm@8.0.0",
  "author": "Cedric Pierre",
  "email": "jirotoh@gmail.com",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --minify --out-dir dist",
    "test": "vitest __tests__",
    "docs": "typedoc --out docs src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedricpierre/fluentity-core.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cedricpierre/fluentity-core/issues"
  },
  "homepage": "https://github.com/cedricpierre/fluentity-core#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.1.4",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "rollup": "^4.12.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}