{
  "entryPoints": ["./src"],
  "out": "docs",
  "exclude": ["**/__tests__/**", "**/*.test.ts", "**/*.spec.ts"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "readme": "README.md",
  "tsconfig": "./tsconfig.docs.json"
} 